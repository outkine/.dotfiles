remove Lock = Caps_Lock
remove Control = Control_L
remove Mod1 = Alt_L
remove Mod4 = Super_L
keysym Caps_Lock = Control_L
keysym Control_L = Tab
keysym Escape = Caps_Lock
keysym Tab = Escape
keysym Alt_L = Super_L
keysym Super_L = Alt_L
add Lock = Caps_Lock
add Control = Control_L
add Mod1 = Alt_L
add Mod4 = Super_L
